{
  "manifest_version": 3,
  "name": "Desktop tab",
  "description": "Desktop tab",
  "version": "0.0.1",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "128": "icon-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["tabs", "topSites"],
  "host_permissions": ["https://t3.gstatic.com/"]
}